<tree string="Test Build"
        colors="If((Eval('test_state') != 'pass') | (Eval('flake_state') != 'pass') | (Eval('coverage_state') == 'error'), 'red', If(Eval('coverage_state') == 'to_improve', 'orange', (If(Eval('coverage_state') == 'acceptable', '#C9D400', 'green'))))">
    <field name="execution"/>
    <field name="component"/>
    <field name="branch"/>
    <field name="revision"/>
    <field name="review"/>
    <field name="lines"/>
    <field name="covered_lines"/>
    <field name="coverage"/>
    <field name="test_state"/>
    <field name="flake_state"/>
    <field name="coverage_state"/>

</tree>
