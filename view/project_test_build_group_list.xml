<tree string="Test Build Group"
        colors="If((Eval('test_state') != 'pass') | (Eval('flake_state') != 'pass') | (Eval('coverage_state') == 'error'), 'red', If(Eval('coverage_state') == 'to_improve', '#FF9D00', (If(Eval('coverage_state') == 'acceptable', '#C9D400', 'green'))))">
    <field name="name"/>
    <field name="db_type"/>
    <field name="start"/>
    <field name="end"/>
    <field name="failfast"/>
    <field name="reviews"/>
    <field name="test_state"/>
    <field name="flake_state"/>
    <field name="coverage_state"/>
</tree>
