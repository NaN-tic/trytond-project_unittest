<tree string="Component State"
        colors="If((Eval('test_state') != 'pass') | (Eval('coverage_state') == 'error'), 'red', If(Eval('coverage_state') == 'to_improve', '#FF9D00', (If(Eval('coverage_state') == 'acceptable', '#C9D400', 'black'))))">
    <field name="rec_name"/>
    <field name="category"/>
    <field name="test_state"/>
    <field name="flake_state"/>
    <field name="coverage_state"/>
</tree>
